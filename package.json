{
  "name": "naa",
  "private": true,
  "version": "2.3.0",
  "description": "基于 Nest + Antd 编写的一款前后端分离的权限管理系统",
  "author": {
    "name": "hexuntao",
    "email": "hexuntao@gmail.com",
    "url": "https://github.com/hexuntao"
  },
  "license": "MIT",
  "homepage": "https://github.com/hexuntao/naa",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/hexuntao/naa.git"
  },
  "bugs": {
    "url": "https://github.com/hexuntao/naa/issues"
  },
  "engines": {
    "node": ">=16.14.1",
    "pnpm": ">=8.6.10"
  },
  "packageManager": "pnpm@8.6.10",
  "scripts": {
    "build": "dotenv -- turbo run build",
    "dev": "dotenv -- turbo run dev",
    "dev:api": "dotenv -- turbo run dev --filter=api",
    "dev:admin": "dotenv -- turbo run dev --filter=admin --filter=api",
    "dev:docs": "dotenv -- turbo run dev --filter=docs",
    "lint": "turbo run lint",
    "format": "prettier --write \"**/*.{ts,tsx,md}\"",
    "upall": "pnpm up --filter @naa/* --latest && pnpm up --latest",
    "prod:api": "cd apps/api && pnpm start:prod",
    "build:api": "cd apps/api && pnpm build",
    "prod:all": "dotenv -- turbo run build && pnpm prod:api",
    "deploy": "pnpm prod:all",
    "start:api": "cd apps/api && pnpm run start:prod",
    "pm2:start": "pm2 start ecosystem.config.js --name naa-api",
    "pm2:stop": "pm2 stop naa-api",
    "pm2:restart": "pm2 restart naa-api",
    "pm2:logs": "pm2 logs naa-api",
    "pm2:status": "pm2 status naa-api",
    "pm2:monit": "pm2 monit",
    "docker:build": "docker build -t naa-api ./apps/api",
    "docker:run": "docker run -d --name naa-api -p 6010:6010 naa-api",
    "docker:stop": "docker stop naa-api && docker rm naa-api"
  },
  "devDependencies": {
    "@naa/eslint-config": "workspace:*",
    "@naa/ts-config": "workspace:*",
    "dotenv-cli": "latest",
    "eslint": "^8.57.1",
    "prettier": "^3.6.2",
    "turbo": "^1.13.4",
    "typescript": "^5.9.3"
  }
}
