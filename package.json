{
  "name": "naa",
  "private": true,
  "version": "1.0.1",
  "description": "基于 NestJs + Antd 编写的一款前后端分离的权限管理系统",
  "author": {
    "name": "hexuntao",
    "email": "hexuntao@gmail.com",
    "url": "https://github.com/hexuntao"
  },
  "license": "MIT",
  "homepage": "https://github.com/hexuntao/naa",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/hexuntao/naa.git"
  },
  "bugs": {
    "url": "https://github.com/hexuntao/naa/issues"
  },
  "engines": {
    "node": ">=16.14.1",
    "pnpm": ">=8.1.0"
  },
  "packageManager": "pnpm@8.6.10",
  "scripts": {
    "build": "turbo run build",
    "dev": "turbo run dev",
    "api:dev": "turbo run dev --filter=api",
    "api:cli": "turbo run cli --cwd=./apps/api --",
    "admin:dev": "turbo run dev --filter=admin --filter=api",
    "web:dev": "turbo run dev --filter=web --filter=api",
    "lint": "turbo run lint",
    "format": "prettier --write \"**/*.{ts,tsx,md}\"",
    "upall": "pnpm up --filter @naa/* --latest && pnpm up --latest"
  },
  "devDependencies": {
    "@naa/eslint-config": "workspace:*",
    "@turbo/gen": "^1.10.12",
    "eslint": "^8.47.0",
    "prettier": "^3.0.1",
    "turbo": "^1.10.13"
  }
}
