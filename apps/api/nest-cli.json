{
  "$schema": "https://json.schemastore.org/nest-cli",
  "collection": "@nestjs/schematics",
  "sourceRoot": "src",
  "compilerOptions": {
    "assets": ["assets/**/*", "config/**/*", "**/*.hbs", "**/*.yaml", "**/*.mapper.xml"],
    "deleteOutDir": true,
    "watchAssets": true,
    "typeCheck": true
  }
}
